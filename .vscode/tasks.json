{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "process",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}\\IntarRepo.sln",
                "-c",
                "Debug"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "run",
            "type": "process",
            "dependsOn": "build",
            "command": "dotnet",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}\\IntarRepo.csproj",
                "--configuration",
                "Debug"
            ],
            "problemMatcher": []
        }
    ]
}
